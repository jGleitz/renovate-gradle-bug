{
    "extends": [
      "config:base"
    ],
    "packageRules": [
      {
        "packagePatterns": [
          "*"
        ],
        "minor": {
          "groupName": "all non-major dependencies",
          "groupSlug": "all-minor-patch"
        }
      }
    ],
    "gradle": {
      "enabled": true,
      "fileMatch": ["^build.gradle$"]
    },
    "prHourlyLimit": 0,
    "prConcurrentLimit": 0
  }
  